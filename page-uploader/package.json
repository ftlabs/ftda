{
  "name": "page-uploader",
  "version": "1.0.0",
  "description": "Upload FTDA data to S3",
  "main": "./app/index.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "build -m --x64",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "build": {
    "appId": "com.ftlabs.ftdauploader",
    "copyright": "Copyright Â© 2017 FTLabs",
    "productName": "FTDA Page Uploader",
    "electronVersion": "1.7.5",
    "icon": "./build/icon",
    "mac": {
      "category": "public.utility.ftda"
    },
    "dmg": {
      "title": "FTDA Upload Installer",
      "icon": "./build/icon.icns",
      "iconSize": 128,
      "contents": [
        {
          "x": 388,
          "y": 160,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 128,
          "y": 160,
          "type": "file",
          "path": ""
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ftlabs/ftda/page-uploader"
  },
  "author": "Sean M. Tracey & Lily Madar",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^19.56.2"
  },
  "dependencies": {
    "@monolambda/s3": "^1.0.2",
    "copy-paste": "^1.3.0",
    "dotenv": "^4.0.0",
    "fs": "0.0.1-security",
    "snyk": "^1.231.1"
  },
  "snyk": true
}
